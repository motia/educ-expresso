    "unfurl.js": "^5.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/formidable": "^1.0.31",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.24",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
